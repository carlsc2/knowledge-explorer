{
  "name": "knowledge-explorer",
  "version": "0.1.0",
  "main": "main.js",
  "productName": "Knowledge Explorer",
  "description": "A desktop app that generated knowledge graph from resources in DBPedia.",
  "homepage": "https://github.com/smiled0g/knowledge-explorer",
  "author": "RPI",
  "scripts": {
    "start": "npm install && ./node_modules/.bin/electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "rellink.knowledgeexplorer",
    "mac": {
      "category": "references"
    },
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/smiled0g/knowledge-explorer/master/icon/Icon.png"
    }
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "crypto": "0.0.3",
    "d3": "^3.5.12",
    "dropzone": "^4.2.0",
    "express": "^4.13.4",
    "jquery": "^2.1.4",
    "node-persist": "0.0.8",
    "querystring": "^0.2.0",
    "request": "^2.65.0",
    "select2-browserify": "^3.4.5",
    "wiki-infobox-parser": "^0.1.11",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "electron": "*",
    "electron-builder": "*"
  }
}
